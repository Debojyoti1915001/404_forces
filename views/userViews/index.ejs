   <div id="myModalAdminSignup" class="modal fade">
      <div class="modal-dialog modal-signup">
         <div class="modal-content">
         <div class="signup-form">
            <form action="/hospital/signup" method="POST">
              <h2>Admin SignUp</h2>
                <div class="form-group">
                 <div class="row">
                    <div class="col"><input type="text" class="form-control" name="institute" placeholder="Institution Name" required="required"></div>
                 </div>
                       	
                </div>
                <div class="form-group">
                <div class="row">
                    <div class="col"><input type="text" class="form-control" name="phoneNumber" placeholder="Phone Number" required="required"></div>
                 </div>
                </div>
                <div class="form-group">
                  <div class="row">
                      <div class="col"><input type="text" class="form-control" name="hospitalName" placeholder="Name" required="required"></div>
                   </div>
                  </div> 
                <div class="form-group">
                   <input type="email" class="form-control" name="email" placeholder="Email" required="required">
                </div>
              <div class="form-group">
                    <input type="password" class="form-control" name="password" placeholder="Password" required="required">
                </div>
              <div class="form-group">
                    <input type="password" class="form-control" name="confirmPwd" placeholder="Confirm Password" required="required">
                </div>        
                <div class="form-group">
                 <label class="form-check-label"><input type="checkbox" required="required"> I accept the <a href="#">Terms of Use</a> &amp; <a href="#">Privacy Policy</a></label>
              </div>
              <div class="form-group">
                    <button type="submit" class="btn btn-success btn-lg btn-block">Register Now</button>
                </div>
            </form>
          
        </div>
      </div>
      </div>
   </div>     
